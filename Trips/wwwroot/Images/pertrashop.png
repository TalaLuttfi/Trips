                        }
                        
                        var onContentLoadedFn = dataset['onContentLoaded'];
                        
                        if(onContentLoadedFn && typeof window[onContentLoadedFn] === 'function'){
                            window[onContentLoadedFn](widgetFeed);
                        }
                        try{
                            var contentEl = document.querySelector('.aff_content');
                            handleParentSize(contentEl);
                            if(ResizeObserver){
                                const resizeObserver = new ResizeObserver(function() {
                                    handleParentSize(contentEl);
                                });
                                resizeObserver.observe(contentEl);
                            }
                            
                        }
                        catch(e){
                            
                        }
                    })
                    .catch(function(err){
                        isLoading = false
                        errorOccured = true;
                        
                        var noShowCallbackFn = dataset['noShowCallback'];
                        
                        if(noShowCallbackFn && typeof window[noShowCallbackFn] === 'function'){
