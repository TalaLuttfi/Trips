                     options.props = getComponentPropsFromDataset(dataset);
                    
                    // in case we need to fethc additional data for widget , dataurl can be passed as URL
                    if (dataset['dataurl']) {
                       options.dataURL = decodeURIComponent(dataset['dataurl']);
                    }
                    
                    // datapath is the path of the node from where the data has to picked form dataurl response
                    if (dataset['datapath']) {
                       options.dataPath = decodeURIComponent(dataset['datapath']);
                    }
                    
                    //  If data has to be passed to the widget in key other than "data"
                    if(dataset['dataaliaskey']){
                        options.dataAliasKey = decodeURIComponent(dataset['dataaliaskey']);
                    }
                    if(globalDataPath){
