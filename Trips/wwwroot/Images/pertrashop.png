                var handleParentSize = function(el){
                    var contentHeight = el.scrollHeight;
                    if(isIframe && contentHeight >= 100){
                        
                        var message = {
                            sentinel: 'amp',
                            type: 'embed-size',
                            height: contentHeight,
                        };
                        window.parent.postMessage(message, '*');    
                    }
