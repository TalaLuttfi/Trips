                window.__aff_script_promises = window.__aff_script_promises || {};
                
                var loadScript = function(src, attributes = {}){
                  var scriptPromise = window.__aff_script_promises[src];
                
                    if (scriptPromise) {
                        return scriptPromise;
                    }
                      
                    if (window.til_affiliates_sdk_v2) {
                        window.__aff_script_promises[src] = Promise.resolve();
                        return window.__aff_script_promises[src];
                    }
                
                  scriptPromise = new Promise(function(resolve, reject){
                    var script = document.getElementById(src); 
                    if(!script){
                        script = document.createElement('script');
                        script.src = src;
                        script.id = src;
                        document.body.appendChild(script);
                    }
                    
                    script.addEventListener('load', function(){
                      setTimeout(function(){
                        resolve();
                      });
                    });
                    script.addEventListener('error', function(e) {
                      reject(e);
                    });
                    
                  });
                
                  window.__aff_script_promises[src] = scriptPromise;
                  return scriptPromise;
                };
                
                var getSafe = function(obj, path, defaultValue = undefined){
                    var travel = (regexp) =>
                        String.prototype.split
                          .call(path, regexp)
                          .filter(Boolean)
                          .reduce(
                            (res, key) => (res !== null && res !== undefined ? res[key] : res),
                            obj
                          )
                    var result = travel(/[,[\]]+?/) || travel(/[,[\].]+?/)
                    return result === undefined || result === obj ? defaultValue : result
                }
            
                var loadSDKAndBindEvents = function(){
                    // bind to gutter ad show/hide events and
