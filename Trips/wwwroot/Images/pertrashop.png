                var getQueryParam = function(name, url = window.location.href) {
                    name = name.replace(/[\[\]]/g, '\\$&');
                    var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                        results = regex.exec(url);
                    if (!results) return null;
                    if (!results[2]) return '';
                    return decodeURIComponent(results[2].replace(/\+/g, ' '));
                }
                
                var getPlatform = function(queryPlatform){
                    var platform = queryPlatform;
                    if(queryPlatform === 'responsive'){
                        var isFrmapp = getQueryParam('frmapp') === 'yes';
                        platform = isMobile() ? (isFrmapp ? 'app' : 'mweb') : (isFrmapp ? 'webapp': 'web');
                    } 
                    return platform;
                }
                
                
                if(!container){
